{
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "prettier/flowtype",
    "prettier/standard"
  ],
  "plugins": [
      "flowtype",
      "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": ["error", { "args": "all" }],
    "flowtype/sort-keys": [
      "error",
      "asc"
    ],
    "flowtype/semi": [
      "error",
      "never"
    ],
    "flowtype/require-parameter-type": 2,
    "prettier/prettier": "error"
  }
}
